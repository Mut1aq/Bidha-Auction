<%- include('../includes/admin-head.ejs') %><link
	rel="stylesheet"
	href="../css/admin.css"
/><link
	href="https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css"
	rel="stylesheet"
/>
<%- include('../includes/admin-sidebar.ejs') %>
<section class="home-section">
	<%- include('../includes/admin-nav.ejs') %>

	<div class="home-content" style="margin: 0 auto">
		<div class="sales-boxes">
			<div class="recent-sales box" style="margin: 0 auto">
				<div class="sales-details" style="margin: 0 auto">
					<form action="/admin-profile" method="post">
						<div class="form">
							<div class="title">Welcome</div>
							<div class="subtitle">
								You can change Profile Info from here!
							</div>
							<div class="input-container ic1">
								<input
									id="username"
									class="input"
									name="admin[username]"
									type="text"
									placeholder=" "
								/>
								<div class="cut"></div>
								<label for="username" class="placeholder"
									>User name</label
								>
							</div>
							<div class="input-container ic2">
								<input
									id="email"
									class="input"
									type="text"
									placeholder=" "
								/>
								<div class="cut cut-short"></div>
								<label for="email" class="placeholder"
									>Email</label
								>
							</div>
							<div class="input-container ic2">
								<input
									id="password"
									class="input"
									type="password"
									name="admin[password]"
									placeholder=" "
								/>
								<div class="cut"></div>
								<label for="password" class="placeholder"
									>Password</label
								>
							</div>

							<button type="text" class="submit">submit</button>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
</section>

<%- include('../includes/admin-end.ejs') %>
